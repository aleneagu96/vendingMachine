CREATE DATABASE vending_machine_container----------------------------------------------------------------------------------------------CREATE TABLE items (
	item_id INT PRIMARY KEY AUTO_INCREMENT,
    name VARCHAR(40),
    price FLOAT,
    quantity INT
    )------------------------------------------------------------------------------------------------
CREATE TABLE money (
	coin_id INT PRIMARY KEY AUTO_INCREMENT,
    coin_type VARCHAR(20),
    quantity int
    )
-----------------------------------------------------------------------------------------------------
INSERT INTO money
(coin_type, quantity)
VALUES
("Penny", 50),
("Nickel", 50),
("Dime", 100),
("Quarter", 150)
